OUTPUT   Queue
SERVER   PUTTY

TABLE Queue
  Id           Char(16)
  Name         Char(64)
  Descr        Char(256)
  InputDriver  Char(16)
  OutputDriver Char(16)
  QueueType    Int      (Message=0, Stream=1, Response=2, Reply=3)
  Priority     int
  Status       int  (Active=0 Inactive=1 MarkForDelete=2)
  USId         Char(16)
  TmStamp      DateTime

KEY QueueKey PRIMARY
  Id

KEY KEY01 UNIQUE
  Name

LINK QueueType QueueType
LINK Drivers   InputDriver
LINK Drivers   OutputDriver

PROC Insert
PROC Update
PROC SelectOne
PROC DeleteOne
PROC Exists
PROC SelectAll
PROC Count

