//SERVER   @DN29_ATLAS
//CONNECT  NPUD00/CONTROL

DATABASE MCPE FLAGS "param=pyodbc"
PACKAGE  putty.bulk
//USING    Bbd.Portal
OUTPUT   Country
SERVER   PUTTY

TABLE Country
  Id      Char(64)
  CodeId  Char(4)
  CurrId  Char(4)
  Status  int   (Active=0 Inactive=1 MarkForDelete=2)
  USId    Char(16)
  Tmstamp timestamp

KEY CountryKey PRIMARY
  Id

KEY KEY01
//## tablespace NPUT001M1 INITIAL 40K NEXT 1M
  CodeId

KEY KEY02
//## tablespace NPUT001M1 INITIAL 40K NEXT 1M
  CurrId

LINK Currency
  CurrId

PROC Insert
PROC Update
PROC SelectOne
PROC DeleteOne

////#Used By Router, IDE
PROC Get//  $PYTHON
INPUT
  Id      Char(64)
OUTPUT (Single)
  CodeId  Char(4)
  CurrId  Char(4)
  Status  int     //# (Active=0 Inactive=1 MarkForDelete=2)
SQLCODE
  Select CodeId, CurrId, Status
  From Country
  Where Id = :Id
ENDCODE

////#Used By Router, IDE
PROC GetByCurrency//  $PYTHON
INPUT
  CurrId  Char(4)
OUTPUT (Single)
  Id      Char(64)
  CodeId  Char(4)
  Status  int     //# (Active=0 Inactive=1 MarkForDelete=2)
SQLCODE
  Select Id, CodeId, Status
  From Country
  Where CurrId = :CurrId
ENDCODE


PROC GetByCodeId
Input
  CodeId Char(4)
Output(single)
  Id     Char(64)
SqlCode
  select trim(Id)
  from   country
  where CodeId = :CodeId
EndCode

