//# used by SaffySnd,tibossnd
PROC Queued
INPUT
  Queue         char(16)
  DayInterval   int
  Status        int
OUTPUT //(Multiple)
  Id            Int
SQLCODE
  SELECT
     Id
  FROM
     Streams
  Where
         Queueid = :Queue
     and Status = :Status
     and DateCreated <= CURRENT_TIMESTAMP
     and DateCreated > DATEADD(day,-:DayInterval, CURRENT_TIMESTAMP)
  ORDER BY Id
ENDCODE

//# used by SaffySndMQ
PROC QueuedEx
INPUT
  Queue         char(16)
  DayInterval   int
  Status        int
OUTPUT //(Multiple)
  Id            Int
  StreamRef     =
SQLCODE
  SELECT
      Id
     ,StreamRef
  FROM
     Streams
  Where
         Queueid = :Queue
     and Status = :Status
     and DateCreated <= CURRENT_TIMESTAMP
     and DateCreated > (CURRENT_TIMESTAMP - :DayInterval)
  ORDER BY Id
ENDCODE

