// Used by front end tester
PROC Queues
INPUT
  DateFrom     DateTime
  DateTo       DateTime
OUTPUT //(Multiple)
  Id Char(16)
  Name Char(64)
  Descr Char(256)
  NoOf int
SQLCODE
  select Q.Id, Q.Name, Q.Descr, count(*) NoOf
  from Queue Q, Streams S
  where S.DateCreated
    between :DateFrom and :DateTo
  and S.QueueId = Q.Id
  group by Q.Id, Q.Name, Q.Descr
ENDCODE

