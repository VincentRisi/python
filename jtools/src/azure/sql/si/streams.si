PROC ByIDGetReference //  $PYTHON
INPUT
    MessageId    int
OUTPUT(Single)
    content      char(64)
SQLCODE
   SELECT 
	content 
   FROM fields
   WHERE messageid = :MessageId AND
	UPPER(tag) = ( 'TXNREFNO')
ENDCODE

