//#Nedpayments i3Batch fix
//# Used By PE Front End
PROC ByQueueAlli3Batch //  $PYTHON
INPUT
   InQueue      char(16)
   InDateFrom   DateTime
   InDateTo     DateTime
OUTPUT//(Multiple)
   Id           int
   MessageId    int
   QueueId      char(16)
   Sourcesysid  char(16)
   Reference    char(64)
   EventQueueId char(16)  NULL
   StreamRef    char(128) NULL
   StreamType   char(16) NULL
   StreamDescr  char(65)  NULL
   MessageLen   int
   MessageType  int(XMLT=0Text=1)
   Priority     int
   Status       int(None=0 Sent=1 Ack=2 Nak=3)
   DateCreated  datetime
   USId         char(16)
   TmStamp      timestamp
SQLCODE
   SELECT
     streams.Id
     ,MessageId
     ,streams.QueueId
     ,message.Sourcesysid
     ,message.Reference
     ,streams.EventQueueId
     ,streams.StreamRef
     ,streams.StreamType
     ,streams.StreamDescr
     ,streams.MessageLen
     ,streams.MessageType
     ,streams.Priority
     ,streams.Status
     ,streams.DateCreated
     ,streams.USId
     ,streams.TmStamp
   FROM
      Streams, message
   WHERE
      message.id = streams.MESSAGEID and message.SOURCESYSID in ('NEDPAYMENT','BANCS','GPS','WACHOVIA','TIBOSFXFTA', 'MERCURY') and 
          (streams.QueueId IN (:InQueue , 'WSS-NPY-WASTE-FP') OR (streams.STREAMTYPE = 'WASTE'))
      AND streams.DateCreated between :InDateFrom
      AND                     :InDateTo
      ORDER BY Id
ENDCODE

