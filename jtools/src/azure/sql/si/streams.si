//# used by AcbFileIn
PROC GetByQRef
INPUT
  StreamRef    char (255)
  QueueId      char (16)
  Status       int

  ID           int
  MessageID    int
  EventQueueID char (16)
  ReplyQueueID char (16)
  ReplyQueue   char (64)
  StreamCount  int
SQLCODE
begin
  Select max(Id)
  into :ID
  From Streams
  Where QueueId = :QueueId
  And   StreamRef = :StreamRef
  And   Status = :Status;

  Select MessageID, EventQueueID
  into :MessageID, :EventQueueID
  From Streams
  Where Id = :ID;


  Select ResponseQueue, StreamCount
  Into :ReplyQueueID, :StreamCount
  From Message Where ID = :MessageID ;


  Select Name
  Into :ReplyQueue
  From Queue Where ID = :ReplyQueueID;
End;
ENDCODE
